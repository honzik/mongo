<h1>Listing measures</h1>

<table>
  <tr>
    <th>Meter</th>
    <th>State</th>
    <th>When</th>
    <th>Pozn</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @measures.each do |measure| %>
  <tr>
    <td><%= measure.meter_id %></td>
    <td><%= measure.state %></td>
    <td><%= measure.when %></td>
    <td><%= measure.pozn %></td>
    <td><%= link_to 'Show', measure %></td>
    <td><%= link_to 'Edit', edit_measure_path(measure) %></td>
    <td><%= link_to 'Destroy', measure, method: :delete %></td>
  </tr>
<% end %>
</table>

